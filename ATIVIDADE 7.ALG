algoritmo "Atividade 6"

var

   T : inteiro
   
inicio

    escrevaL("Qual é a atual temperatura ambiente?")
    leia (T)
    
    SE T>=25 ENTAO
    escrevaL ("Nossa! Está muito quente!")
    
    SENAO
    
    SE T<25 ENTAO
    escrevaL("Ok, está agradável")
    
    FIMSE
     FIMSE
    
fimalgoritmo