algoritmo "Atividade 1"
// lista de atividades
var
nome : caractere
sobrenome : caractere
soma : caractere
inicio
    escreva("Qual seu primeiro nome? ")
    leia(nome)
    escreva("Qual seu sobrenome? ")
    leia(sobrenome)
    
    soma <- nome + sobrenome
    escreva("Seu nome é " , soma )
    
fimalgoritmo